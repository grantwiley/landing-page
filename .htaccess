RewriteEngine On

# Ensure proper base
RewriteBase /

# Redirect /quote to quote.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^quote/?$ quote.html [NC,L]

# Redirect /contact to contact.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^contact/?$ contact.html [NC,L]

# Handle 404 errors
ErrorDocument 404 /404.html